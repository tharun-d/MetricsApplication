@model IEnumerable<MetricsApplication.BussinessEntity.NotFilled>

@{
    ViewBag.Title = "NotFilled";
    Layout = "~/Views/Analysis/_AnalysisLayout.cshtml";
}

<h2>NotFilled</h2>


<table class="table">
    <tr>
        <th>
            Employee Name
        </th>
        <th>
            Email
        </th>
        <th>
            Application Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.applicationName)
        </td>
    </tr>
}

</table>
